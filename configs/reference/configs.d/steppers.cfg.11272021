enable_pin: !P1.16
microsteps: 16
rotation_distance: 8
full_steps_per_rotation: 200    # The number of full steps for one rotation of the stepper motor. 200 for 1.8 motor or 400 for 0.9 motor.

[tmc2208 stepper_z1]
uart_pin: P1.1
run_current: 0.650
hold_current: 0.450
stealthchop_threshold: 0 # 0 = disabled
interpolate: True

[extruder]
step_pin: P2.13
dir_pin: !P0.11
enable_pin: !P2.12
microsteps: 16
gear_ratio: 50:17
rotation_distance: 22.62
full_steps_per_rotation: 200 # 200 for 1.8 Deg motor, 400 for 0.9 Deg motor
nozzle_diameter: 0.400
filament_diameter: 1.750
heater_pin: P2.7
sensor_type: ATC Semitec 104GT-2
sensor_pin: P0.24
#control: pid # Saved config using PID TUNE
#pid_Kp: 22.2
#pid_Ki: 1.08
#pid_Kd: 114
min_temp: 0
max_temp: 260
max_extrude_only_distance: 900.0
max_extrude_only_velocity: 60

[tmc2208 extruder]
uart_pin: P1.4
run_current: 0.800
hold_current: 0.500
interpolate: True
stealthchop_threshold: 0 # 0 = disabled
interpolate: True
